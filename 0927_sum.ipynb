    "def summ(n):\n",
    "    tmp = []\n",
    "    for i in range(1,n+1):\n",
    "        if n % i == 0:\n",
    "            tmp.append(i)\n",
    "        else: \n",
    "            pass\n",
    "    return sum(tmp)"
   ]
